#!/usr/bin/python3
"""validUTF8."""


def validUTF8(data):
    """Determine if a given data set represents a valid UTF-8 encoding."""
    valid = True
    for i in range(len(data)):
        char = data[i]
        msb = char >> 7
        print (msb)
        if (msb) :
            print(char)
    return True
